{
  "name": "vscode-wdio",
  "displayName": "vscode-wdio",
  "description": "WebdriverIO test runner for vscode",
  "version": "0.0.3",
  "publisher": "murbani",
  "repository": "https://github.com/marcellourbani/vscode-wdio",
  "keywords": [
    "test",
    "wdio",
    "Webdriver.IO"
  ],
  "license": "MIT",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/wdio.conf.js",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "devDependencies": {
    "@babel/types": "^7.19.4",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.0",
    "@types/minimatch": "^5.1.2",
    "@types/node": "16.x",
    "@types/vscode": "^1.72.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@babel/parser": "^7.19.6",
    "ansi-regex": "^5.0.1",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.19",
    "io-ts-reporters": "^2.0.1",
    "minimatch": "^5.1.0",
    "short-uuid": "^4.2.2"
  }
}
